{
  "name": "@juniper/db",
  "version": "0.0.1",
  "description": "The database scaffolding for juniper stack",
  "author": "Daniel <daniel.ashcraft@ofashandfire.com>",
  "homepage": "https://github.com/juniper-garden/juniper-tech-microservices#readme",
  "license": "MIT",
  "main": "build/lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juniper-garden/juniper-tech-microservices.git"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "dev": "nodemon",
    "start": "ts-node tsconfig.json lib/index.ts",
    "start:prod": "ts-node lib/index.ts --transpileOnly",
    "db:create": "npx sequelize-cli db:create",
    "db:seed": "npx sequelize-cli db:seed",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate"
  },
  "bugs": {
    "url": "https://github.com/juniper-garden/juniper-tech-microservices/issues"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/sequelize": "^4.28.10",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.10",
    "dotenv": "^16.0.0",
    "pg": "^8.7.3",
    "sequelize": "^6.12.0",
    "sequelize-date-no-tz-postgres": "^1.0.0"
  }
}
